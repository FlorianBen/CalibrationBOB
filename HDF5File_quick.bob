    <name>Label_AutoIncrement</name>
    <text>Auto Increment</text>
    <x>10</x>
    <y>200</y>
  </widget>
  <widget type="label" version="2.0.0">
    <name>Label_AutoSave</name>
    <text>Auto Save</text>
    <x>10</x>
    <y>140</y>
  </widget>
