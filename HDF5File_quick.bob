<?xml version="1.0" encoding="UTF-8"?>
<display version="2.0.0">
  <name>HDF5 File configuration</name>
  <width>480</width>
  <height>360</height>
  <widget type="label" version="2.0.0">
    <name>Label_Title</name>
    <class>SECTION</class>
    <text>HDF5 File configuration</text>
    <width>480</width>
    <height>31</height>
    <font use_class="true">
      <font name="Default Bold" family="Liberation Sans" style="BOLD" size="14.0">
      </font>
    </font>
    <foreground_color use_class="true">
      <color name="Text" red="0" green="0" blue="0">
      </color>
    </foreground_color>
    <transparent use_class="true">true</transparent>
  </widget>
  <widget type="label" version="2.0.0">
    <name>Label_FilePath</name>
    <text>Filepath</text>
    <x>10</x>
    <y>20</y>
  </widget>
  <widget type="label" version="2.0.0">
    <name>Label_FileName</name>
    <text>Filename</text>
    <x>10</x>
    <y>50</y>
  </widget>
  <widget type="label" version="2.0.0">
    <name>Label_FileTemplate</name>
    <text>Template file</text>
    <x>10</x>
    <y>80</y>
  </widget>
