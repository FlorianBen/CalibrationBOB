    <name>Action Button</name>
    <actions>
      <action type="write_pv">
        <pv_name>$(P)$(R)Capture</pv_name>
        <value>1</value>
        <description>WritePV</description>
      </action>
    </actions>
    <pv_name>$(P)$(R)Capture</pv_name>
    <text>Record</text>
    <x>10</x>
    <y>290</y>
    <height>20</height>
    <tooltip>$(actions)</tooltip>
  </widget>
